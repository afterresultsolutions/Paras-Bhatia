// Advanced Animations for AR Solutions

// GSAP-like animation functions (vanilla JS implementation)
class AnimationController {
    constructor() {
        this.animations = [];
        this.init();
    }

    init() {
        this.initScrollTriggers();
        this.initHoverEffects();
        this.initLoadAnimations();
    }

    // Scroll-triggered animations
    initScrollTriggers() {
        const observerOptions = {
            threshold: 0.2,
            rootMargin: '0px 0px -100px 0px'
        };

        const observer = new IntersectionObserver((entries) => {
            entries.forEach(entry => {
                if (entry.isIntersecting) {
                    this.animateElement(entry.target);
                }
            });
        }, observerOptions);

        // Elements to animate on scroll
        const elementsToAnimate = document.querySelectorAll(`
            .about-card,
            .service-card,
            .portfolio-item,
            .stat-item,
            .contact-method,
            .section-header
        `);

        elementsToAnimate.forEach(el => {
            el.style.opacity = '0';
            el.style.transform = 'translateY(30px)';
            observer.observe(el);
        });
    }

    animateElement(element) {
        const delay = Array.from(element.parentNode.children).indexOf(element) * 100;
        
        setTimeout(() => {
            element.style.transition = 'all 0.8s cubic-bezier(0.4, 0, 0.2, 1)';
            element.style.opacity = '1';
            element.style.transform = 'translateY(0)';
        }, delay);
    }

    // Hover effects
    initHoverEffects() {
        // Service cards hover effect
        const serviceCards = document.querySelectorAll('.service-card');
        serviceCards.forEach(card => {
            card.addEventListener('mouseenter', () => {
                this.scaleElement(card, 1.05);
                this.addGlow(card);
            });

            card.addEventListener('mouseleave', () => {
                this.scaleElement(card, 1);
                this.removeGlow(card);
            });
        });

        // Button hover effects
        const buttons = document.querySelectorAll('.btn');
        buttons.forEach(btn => {
            btn.addEventListener('mouseenter', () => {
                this.addButtonEffect(btn);
            });

            btn.addEventListener('mouseleave', () => {
                this.removeButtonEffect(btn);
            });
        });

        // Navigation hover effects
        const navLinks = document.querySelectorAll('.nav-link');
        navLinks.forEach(link => {
            link.addEventListener('mouseenter', () => {
                this.addNavHover(link);
            });

            link.addEventListener('mouseleave', () => {
                this.removeNavHover(link);
            });
        });
    }

    scaleElement(element, scale) {
        element.style.transform = `scale(${scale})`;
    }

    addGlow(element) {
        element.style.boxShadow = '0 20px 40px rgba(99, 102, 241, 0.2)';
    }

    removeGlow(element) {
        element.style.boxShadow = '0 4px 6px -1px rgba(0, 0, 0, 0.1)';
    }

    addButtonEffect(button) {
        button.style.transform = 'translateY(-3px) scale(1.05)';
        button.style.boxShadow = '0 10px 20px rgba(99, 102, 241, 0.3)';
    }

    removeButtonEffect(button) {
        button.style.transform = 'translateY(0) scale(1)';
        button.style.boxShadow = '0 4px 6px -1px rgba(0, 0, 0, 0.1)';
    }

    addNavHover(link) {
        link.style.transform = 'translateY(-2px)';
    }

    removeNavHover(link) {
        if (!link.classList.contains('active')) {
            link.style.transform = 'translateY(0)';
        }
    }

